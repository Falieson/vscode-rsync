{
  "name": "sync-rsync",
  "displayName": "Sync - Rsync",
  "description": "Sync with remote using rsync",
  "version": "0.0.1",
  "publisher": "vscode-ext",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.syncDown"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.syncDown",
        "title": "Sync Down"
      }
    ],
    "configuration": {
      "type":"object",
      "title": "Sync - Rsync configuration",
      "properties": {
        "sync-rsync.remote": {
          "type":"string",
          "default": null,
          "description": "Remote Location (user@host:path)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/rsync": "^0.4.28",
    "rsync": "^0.5.0"
  }
}
