{
  "name": "sync-rsync",
  "displayName": "Sync-Rsync",
  "description": "Sync with remote using rsync",
  "version": "0.12.0",
  "publisher": "leshniak",
  "repository": {
    "type": "git",
    "url": "https://github.com/leshniak/vscode-rsync"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "sync-rsync.syncDown",
        "title": "Sync - Rsync: Remote to Local"
      },
      {
        "command": "sync-rsync.syncUp",
        "title": "Sync - Rsync: Local to Remote"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Sync - Rsync configuration",
      "properties": {
        "sync-rsync.remote": {
          "type": "string",
          "default": null,
          "description": "Remote Location (user@host:path) (must end in path separator)"
        },
        "sync-rsync.local": {
          "type": "string",
          "default": null,
          "description": "Local location defaults to workspace (must end in path separator)"
        },
        "sync-rsync.delete": {
          "type": "boolean",
          "default": false,
          "description": "Delete false during sync"
        },
        "sync-rsync.flags": {
          "type": "string",
          "default": "rlptzv",
          "description": "Rsync Flags"
        },
        "sync-rsync.exclude": {
          "type": "array",
          "default": [
            ".git",
            ".vscode"
          ],
          "description": "Rsync exclude patterns",
          "items": {
            "type": "string",
            "title": "Rsync exclude pattern"
          }
        },
        "sync-rsync.onSave": {
          "type": "boolean",
          "default": false,
          "description": "Sync File on Save"
        },
        "sync-rsync.autoShowOutput": {
          "type": "boolean",
          "default": false,
          "description": "Auto show rsync output when rsync is working"
        },
        "sync-rsync.autoHideOutput": {
          "type": "boolean",
          "default": false,
          "description": "Auto hide rsync output when rsync is done"
        },
        "sync-rsync.shell": {
          "type": "string",
          "description": "Rsync's -e option e.g. ssh -p 1234"
        },
        "sync-rsync.chmod": {
          "type": "string",
          "description": "Rsync's --chmod option"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "lodash.debounce": "^4.0.8",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "rsync": "^0.6.1"
  }
}
